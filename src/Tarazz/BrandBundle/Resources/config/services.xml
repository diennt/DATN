<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="tarazz_brand.brand_grid.class">Tarazz\BrandBundle\Grid\BrandGrid</parameter>
    </parameters>

    <services>
        <service id="tarazz_brand.brand_grid" class="%tarazz_brand.brand_grid.class%" scope="prototype">
            <argument type="service" id="service_container" />
            <argument type="service" id="grid" />
        </service>

        <service id="tarazz_brand.brand_grid.test" class="Tarazz\BrandBundle\Services\TestService">
            <argument type="service" id="service_container" />
        </service>
    </services>
</container>
